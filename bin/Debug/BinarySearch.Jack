function int BinarySearch(array a, int key, int length){
	var int start;
	var int end;
	var int median;
	
	let start = 0;
	let end = length;
	
	while( start < end - 1){
		let median = (start + length) / 2;
		if(key = a[median])
			return key;
		if(key < a[median])
			let end = median;
		if(key > a[median])
			let start = median + 1;
	}
	return 0-1;
}
